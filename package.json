{
  "name": "stub",
  "version": "1.0.0",
  "description": "Cinema6 Angular Stub.",
  "dependencies": {},
  "devDependencies": {
    "grunt": "~0.4.1",
    "load-grunt-config": "git://github.com/firstandthird/load-grunt-config",
    "grunt-git-assist": "git+ssh://git@github.com:cinema6/grunt-git-assist",
    "c6-sandbox": "git+ssh://git@github.com:cinema6/sandbox",
    "connect-livereload": "~0.3.0",
    "grunt-contrib-connect": "~0.5.0",
    "grunt-open": "~0.2.2",
    "grunt-contrib-watch": "~0.5.3"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "readmeFilename": "README.md",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/cinema6/stub.git"
  },
  "c6Settings": {
    "appUrl": "http://<%= env.myIP %>:<%= settings.appPort %>/",
    "appDir": "app",
    "distDir": "dist",
    "awsJSON": ".aws.json",
    "appPort": 8000,
    "sandboxPort": 9000,
    "collateralDir": "siteContent",
    "experiencesJSON": "<%= settings.appDir %>/assets/mock/experiences.json",
    "s3": {
      "test": {
        "bucket": "c6.dev",
        "collateral": "media/src/site/collateral/"
      },
      "production": {
        "bucket": "cinema6.com-etc",
        "collateral": "collateral/"
      }
    }
  }
}
